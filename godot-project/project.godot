; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "EditorInspectorPlugin",
"class": "CameraInspectorPlugin",
"language": "GDScript",
"path": "res://addons/qol-camera/inspector_plugin.gd"
}, {
"base": "EditorProperty",
"class": "ViewLockProperty",
"language": "GDScript",
"path": "res://addons/qol-camera/view_lock_property.gd"
} ]
_global_script_class_icons={
"CameraInspectorPlugin": "",
"ViewLockProperty": ""
}

[application]

config/name="3D Evolution"
run/main_scene="res://MainScene.tscn"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/qol-camera/plugin.cfg" )

[gdnative]

singletons=[  ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
